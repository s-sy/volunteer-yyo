/* pages/preach/preach-list/index.wxss */
.UCenter-bg {
  background-image: url(https://img.tmyomi.com/static/preach.png);
  background-size: cover;
  background-position: center center;
  width: auto;
  height: 350rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}
.preach-list-view{
  width: 690rpx;
  border-radius: 10rpx;
  position: relative;
  z-index: 99;
  background: #FFFfff;
  margin: -30rpx 30rpx 45rpx;
}
.one{
  border-radius: 80rpx;
  height: 45rpx;
  width:50rpx;
  /* border:solid 1rpx black; */
  text-align: center;
  line-height: 40rpx;
  background: red;
  color:#fcff31;
}
.one-text{
  margin-left: 20rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: red;
}
.xian{
 border-bottom:solid 1rpx black;
  height: 20rpx;
}